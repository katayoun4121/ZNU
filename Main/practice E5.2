package E5;
import java.util.Scanner;
public class practice1 {
    public static void main(String[] args){
        System.out.println("multiplication table: ");
        System.out.println("--------------------------");
        for(int i =1;i<=10;i++){
            for (int j=1 ;j<=10; j++){
                System.out.printf("%5d",i*j);
            }
            System.out.println();
        }
    }
}
